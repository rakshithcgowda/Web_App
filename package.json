    "@tanstack/react-router": "^1.131.50",
    "@types/react-router-dom": "^5.3.3",
    "@vercel/postgres": "^0.10.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.22",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
