{
  "name": "x",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "nodemon --exec tsx server/index.ts",
    "build": "tsc -b && vite build",
    "build:server": "tsc server/index.ts --outDir dist-server --target ES2020 --module commonjs --esModuleInterop --allowSyntheticDefaultImports --strict",
    "start": "node dist-server/index.js",
    "start:server": "tsx server/index.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "vercel-build": "npm run build"
  },
